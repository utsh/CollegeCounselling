<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.utkarsh.collegecounselling" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CollegeListBtech"
            android:label="@string/title_activity_college"
            android:parentActivityName=".HomePage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.utkarsh.collegecounselling.HomePage" />
        </activity>
        <activity
            android:name=".CollegeListBba"
            android:label="@string/title_activity_college_list_bba"
            android:parentActivityName=".HomePage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.utkarsh.collegecounselling.HomePage" />
        </activity>
        <activity
            android:name=".CollegeListLlb"
            android:label="@string/title_activity_collge_list_llb"
            android:parentActivityName=".HomePage" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.utkarsh.collegecounselling.HomePage" />
        </activity>
    </application>

</manifest>
